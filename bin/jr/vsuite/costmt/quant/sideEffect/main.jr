public class main {
    private static int sideEffect = 10;
    public static void main(String [] args){
	System.out.println("begin");
	co ((int i = 1; i <= funny(); i++),(int j = 2; j <= 3; j++)) call p(i,j) {
	    System.out.println("after: "+i+" "+j); sideEffect++;
	}
	System.out.println("done "+sideEffect);
    }

    // prints both args
    private static op void p(int i, int j) {
	System.out.println("p: "+i+" "+j);
    }
    private static int funny() {
	sideEffect++;
	return 4;
    }
}
