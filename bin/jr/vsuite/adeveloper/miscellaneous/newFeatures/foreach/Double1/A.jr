public class A {
	
  public static void main(String[] args) {
    double [] d = new double[] {0.3, .99, 8.88};

    printemout(d);

    System.out.println(addemup(d));    

    printemout(d);
  }

  static double addemup(double[] ds) {
    double total = 0;
    for (double d: ds)
      total += d;
    return total;
  }

  static void printemout (double[] ds) {
    for(double item : ds) {
      System.out.println(item);
    }
  }

}
