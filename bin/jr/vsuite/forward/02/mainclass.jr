public class mainclass
{
    public op int p1(int);
    remote fwd2 f;
    public mainclass()
    {
	f = new remote fwd2();
	call p();
    }

    public int p1(int j)
    {
	int result = 0;
	    forward f.p2(j);
	    j = 1111;
	    result = 1000;
	    send f.p2wait();
	return result;
    }

    public op void p();
    public void p()
    {
	int i = 0;
	System.out.println(p1(i));
	System.out.println(i);
    }
}
