import edu.ucdavis.jr.JR;

public class Scheduler {

    public Scheduler() {
	System.out.println("Scheduler ready");
    };

    public op void go_ahead();
    
    public op void request();
    public void request() {
	    System.out.println("got request");
	    System.out.println("awaiting go_ahead");
	    receive go_ahead();
	    System.out.println("got go-ahead");
    }
    
}
