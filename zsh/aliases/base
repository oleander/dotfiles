# Jumping to a dir
alias dot='cd ~/.dotfiles'

# Other
alias p='ps aux | grep'
alias history='fc -il 1'
alias h='history | grep'
alias reload='source ~/.zshrc'

function most() {
  history | awk '{CMD[$4]++;count++;}END { for (a in CMD)print CMD[a] " " CMD[a]/count*100 "% " a;}' | grep -v "./" | column -c4 -s " " -t | sort -nr | nl |  head -n10
}