alias gp='git push'
alias gpp='git pull'
alias gs='git status --ignore-submodules -s -b'
alias gcp='git cherry-pick'
alias gd='git diff --stat'
alias gb='git branch'
alias gl='git log --format="%h by %C(red)%an%Creset, %ar %C(green)%s" --decorate --all'
alias gdd='git diff'
alias gg='git-gg'
alias gcc='git commit -v -m'
alias gc='git-verify-checkout'