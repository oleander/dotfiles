# Aliases
export PROJECT="/Users/oleander/Documents/Projekt"
export DOT=$PROJECT/dotfiles

alias cr='cd $PROJECT'
alias r='cr && cd radiofy.fm'
alias dd='cd ~/Desktop'
alias flush='dscacheutil -flushcache'
alias oi='open-issues'
alias update='sudo /usr/libexec/locate.updatedb'
alias dot='cd $DOT'
alias treload='cap production test && reload'
alias reload_source='source ~/.zshrc'
alias reload='reload_iterm'
alias rm=trash

# Exports
export EDITOR="atom"
export JAVA_HOME=$(/usr/libexec/java_home)
export GHC_DOT_APP="/Applications/ghc-7.8.4.app"
export SSL_CERT_FILE=/usr/local/etc/openssl/cacert.pem

export PATH="${PATH}:${HOME}/.cabal/bin:${GHC_DOT_APP}/Contents/bin"
export PATH=$HOME/.rvm/bin:/usr/local/share/npm/bin:$PATH
export PATH=$PATH:/opt/local/bin/
export PATH=$PATH:/usr/local/bin:/usr/local/sbin
export PATH=$PATH:/usr/local/bin:/usr/local/sbin
export PATH=$PATH:/usr/local/mysql/bin:/usr/local/git/bin
export PATH=$PATH:/opt/local/bin:/opt/local/sbin
export PATH=$PATH:/Applications/Postgres93.app/Contents/MacOS/bin
export PATH="$PATH:$(brew --prefix coreutils)/libexec/gnubin"
